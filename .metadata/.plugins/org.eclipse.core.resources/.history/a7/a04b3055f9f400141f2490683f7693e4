package crud;

import java.sql.*;

public class MySQLCreator extends DBCreator {

	Connection con;
	
	public void initConnection(){
		try {
			con = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/employees?user=root&password=password");

		} catch (Exception e) {
			e.printStackTrace();
		}
	}
	
